<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundScoreView">
        <attr name="bgStyle">
            <enum name="roundType" value="1" />
            <enum name="circleType" value="2" />
        </attr>
        <attr name="text_size" format="integer" />
    </declare-styleable>
    <declare-styleable name="StatefulLayout">
        <attr name="defaultState" format="enum">
            <enum name="content" value="0" />
            <enum name="progress" value="1" />
            <enum name="offline" value="2" />
            <enum name="empty" value="3" />
        </attr>
        <attr name="progressLayout" format="reference" />
        <attr name="offlineLayout" format="reference" />
        <attr name="emptyLayout" format="reference" />
    </declare-styleable>
</resources>